<%= form_for(@profil) do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>
  <%= f.label :actual_job %>
  <%= f.text_field :actual_job %>
  <%= f.label :company %>
  <%= f.text_field :company %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :dream_job %>
  <%= f.text_field :dream_job %>
  <%= f.submit %>
<% end %>
